    "@rsmdc/menu": "^1.1.15",
    "@rsmdc/notched-outline": "^1.1.6",
    "@rsmdc/ripple": "^1.1.15",
    "@rsmdc/rtl": "^1.1.0",
    "@rsmdc/shape": "^1.1.0",
    "@rsmdc/theme": "^1.1.0",
    "@rsmdc/typography": "^1.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7"
